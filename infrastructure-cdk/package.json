{
  "name": "sjc1990app-infrastructure",
  "version": "1.0.0",
  "description": "AWS CDK infrastructure for sjc1990app",
  "bin": {
    "infrastructure-cdk": "bin/infrastructure-cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "cdk deploy --all --require-approval never",
    "deploy:dev": "cdk deploy --all --context stage=dev",
    "deploy:staging": "cdk deploy --all --context stage=staging",
    "deploy:prod": "cdk deploy --all --context stage=prod",
    "diff": "cdk diff",
    "synth": "cdk synth",
    "destroy": "cdk destroy --all"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "20.10.0",
    "aws-cdk": "^2.110.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.3.0"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.110.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
